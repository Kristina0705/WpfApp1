<Window x:Class="WpfApp1.SamTest" 
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="SamTest" Height="450" Width="800">
    <Grid>
        <Label x:Name="label1" Content="Label" HorizontalAlignment="Left" VerticalAlignment="Top" Width="118" Margin="10,10,0,0"/>
        <Label x:Name="questionCountLabel" Content="Label" HorizontalAlignment="Left" VerticalAlignment="Top" RenderTransformOrigin="5.854,2.579" Margin="662,17,0,0" Width="81"/>
        <StackPanel x:Name="testPanel" HorizontalAlignment="Left" Height="140" VerticalAlignment="Top" Width="706" Margin="37,55,0,0">
            <Label x:Name="questionText" Content="Label"/>
            <RadioButton x:Name="answer1" Content="RadioButton"/>
            <RadioButton x:Name="answer2" Content="RadioButton"/>
            <RadioButton x:Name="answer3" Content="RadioButton"/>
            <RadioButton x:Name="answer4" Content="RadioButton"/>
            <RadioButton x:Name="answer5" Content="RadioButton"/>
        </StackPanel>
        <TextBlock x:Name="timeLabel" Text="Время: 00:00" Margin="298,245,332,150"/>

    </Grid>
</Window>
